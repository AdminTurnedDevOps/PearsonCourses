{
  "name": "atlassian-openapi",
  "version": "1.0.20",
  "description": "This is a package that lets you deal with the Atlassian flavour of the OpenAPI 3.0 specification.",
  "repository": "github:robertmassaioli/atlassian-openapi",
  "keywords": [
    "atlassian",
    "openapi",
    "swagger"
  ],
  "author": "Robert Massaioli",
  "license": "MIT",
  "main": "lib/index",
  "typings": "lib/index",
  "files": [
    "lib/**",
    "swagger.v3.json"
  ],
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch",
    "lint": "tslint 'src/**/*.ts*'",
    "build": "tsc --project .",
    "prepare": "npm run build"
  },
  "dependencies": {
    "jsonpointer": "^5.0.0",
    "urijs": "^1.19.10"
  },
  "devDependencies": {
    "@types/jest": "^23.3.14",
    "@types/urijs": "^1.15.33",
    "husky": "^3.0.5",
    "jest": "^23.6.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.10.0",
    "typescript": "^3.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
