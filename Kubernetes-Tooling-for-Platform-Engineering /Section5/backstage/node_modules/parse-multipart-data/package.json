{
  "name": "parse-multipart-data",
  "version": "1.5.0",
  "description": "A javascript/nodejs multipart/form-data parser which operates on raw data.",
  "main": "dist/multipart.js",
  "author": "Ignacio Mazzara (nachomazzara@gmail.com)",
  "license": "MIT",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc --project tsconfig.json",
    "test": "nyc mocha --require ts-node/register test/*.spec.ts",
    "semantic-release": "semantic-release"
  },
  "nyc": {
    "all": true,
    "include": [
      "src"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nachomazzara/parse-multipart-data.git"
  },
  "keywords": [
    "multipart/form-data",
    "form",
    "fileuploader"
  ],
  "bugs": {
    "url": "https://github.com/nachomazzara/parse-multipart-data/issues"
  },
  "homepage": "https://github.com/nachomazzara/parse-multipart-data#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.23",
    "chai": "^4.3.6",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "prettier": "^2.6.2",
    "semantic-release": "^19.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "semi": false,
    "trailingComma": "none"
  }
}
