  },
  "author": "Kyle Shockey <kyleshockey@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-jest": "^21.2.0",
    "babel-preset-es2015": "^6.3.13",
    "jest": "^21.2.1",
    "release-it": "^4.1.1",
    "tape": "^4.2.2"
  },
  "dependencies": {
    "repeat-string": "^1.5.2"
  },
  "jest": {
    "testMatch": [
      "**/test/*.js"
