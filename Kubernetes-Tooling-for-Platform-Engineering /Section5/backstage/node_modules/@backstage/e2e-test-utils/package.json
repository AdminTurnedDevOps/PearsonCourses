{
  "name": "@backstage/e2e-test-utils",
  "description": "Shared end-to-end test utilities Backstage",
  "version": "0.1.1",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
