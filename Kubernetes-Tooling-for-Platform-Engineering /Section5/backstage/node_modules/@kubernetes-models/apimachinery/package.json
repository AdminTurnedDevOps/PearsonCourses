{
  "name": "@kubernetes-models/apimachinery",
  "version": "2.0.2",
  "description": "Types for Kubernetes API objects",
  "repository": {
    "type": "git",
    "url": "https://github.com/tommy351/kubernetes-models-ts.git"
  },
  "homepage": "https://github.com/tommy351/kubernetes-models-ts/tree/master/packages/apimachinery",
  "author": "Tommy Chen <tommy351@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "module": "index.mjs",
  "types": "index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "directory": "dist",
    "linkDirectory": true
  },
  "keywords": [
    "kubernetes",
    "kubernetes-models",
    "apimachinery"
  ],
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@swc/helpers": "^0.5.8",
    "@kubernetes-models/base": "^5.0.1",
    "@kubernetes-models/validate": "^4.0.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.7.23",
    "lodash": "^4.17.21",
    "openapi-types": "^12.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "@kubernetes-models/openapi-generate": "^5.0.2",
    "@kubernetes-models/publish-scripts": "^1.2.0",
    "@kubernetes-models/string-util": "^3.0.1",
    "@kubernetes-models/read-input": "^3.1.1"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.js"
    },
    "./_schemas/*": {
      "types": "./_schemas/*.d.ts",
      "import": "./_schemas/*.mjs",
      "require": "./_schemas/*.js"
    },
    "./api": {
      "types": "./api/index.d.ts",
      "import": "./api/index.mjs",
      "require": "./api/index.js"
    },
    "./api/resource": {
      "types": "./api/resource/index.d.ts",
      "import": "./api/resource/index.mjs",
      "require": "./api/resource/index.js"
    },
    "./api/resource/*": {
      "types": "./api/resource/*.d.ts",
      "import": "./api/resource/*.mjs",
      "require": "./api/resource/*.js"
    },
    "./apis": {
      "types": "./apis/index.d.ts",
      "import": "./apis/index.mjs",
      "require": "./apis/index.js"
    },
    "./apis/meta": {
      "types": "./apis/meta/index.d.ts",
      "import": "./apis/meta/index.mjs",
      "require": "./apis/meta/index.js"
    },
    "./apis/meta/v1": {
      "types": "./apis/meta/v1/index.d.ts",
      "import": "./apis/meta/v1/index.mjs",
      "require": "./apis/meta/v1/index.js"
    },
    "./apis/meta/v1/*": {
      "types": "./apis/meta/v1/*.d.ts",
      "import": "./apis/meta/v1/*.mjs",
      "require": "./apis/meta/v1/*.js"
    },
    "./package.json": "./package.json",
    "./runtime": {
      "types": "./runtime/index.d.ts",
      "import": "./runtime/index.mjs",
      "require": "./runtime/index.js"
    },
    "./runtime/*": {
      "types": "./runtime/*.d.ts",
      "import": "./runtime/*.mjs",
      "require": "./runtime/*.js"
    },
    "./util": {
      "types": "./util/index.d.ts",
      "import": "./util/index.mjs",
      "require": "./util/index.js"
    },
    "./util/intstr": {
      "types": "./util/intstr/index.d.ts",
      "import": "./util/intstr/index.mjs",
      "require": "./util/intstr/index.js"
    },
    "./util/intstr/*": {
      "types": "./util/intstr/*.d.ts",
      "import": "./util/intstr/*.mjs",
      "require": "./util/intstr/*.js"
    },
    "./version": {
      "types": "./version/index.d.ts",
      "import": "./version/index.mjs",
      "require": "./version/index.js"
    },
    "./version/*": {
      "types": "./version/*.d.ts",
      "import": "./version/*.mjs",
      "require": "./version/*.js"
    }
  },
  "scripts": {
    "build": "ts-node scripts/generate.ts && publish-scripts build --include-hidden"
  }
}