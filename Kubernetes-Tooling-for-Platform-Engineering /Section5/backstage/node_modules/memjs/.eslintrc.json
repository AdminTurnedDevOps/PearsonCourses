{
    "rules": {
        "indent": [
            2,
            2
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ]
    },
    "globals": {
        "Promise": "readonly"
    },
    "env": {
        "node": true,
        "browser": true
    },
    "extends": "eslint:recommended"
}
