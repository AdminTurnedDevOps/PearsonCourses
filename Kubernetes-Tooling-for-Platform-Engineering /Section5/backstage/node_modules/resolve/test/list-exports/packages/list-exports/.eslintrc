{
	"root": true,

	"extends": "@ljharb/eslint-config/node/10",

	"rules": {
		"complexity": 0,
		"eqeqeq": [2, "allow-null"],
		"func-style": 1,
		"id-length": 0,
		"max-lines": 0,
		"max-lines-per-function": 0,
		"max-params": 1,
		"max-statements": 1,
		"multiline-comment-style": 0,
		"new-cap": [2, {
			"capIsNewExceptions": ["GetIntrinsic"],
		}],
		"sort-keys": 0,
	},
}
